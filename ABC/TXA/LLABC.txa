NAME ABC CloseCurrentWindow
[INSTANCE]
INSTANCE 6
[WINDOW]
QuickWindow WINDOW('Update the RepLay File'),AT(,,347,91),FONT('MS Sans Serif',8,,),CENTER,IMM,HLP('CopyRptLayout'), |
         SYSTEM,GRAY,RESIZE,MDI
       SHEET,AT(4,4,341,63),USE(?CurrentTab),WIZARD,#ORIG(CurrentTab)
         TAB('General'),USE(?Tab:1)
           PROMPT('Layout Will Be Based On:'),AT(8,18),USE(?Prompt2),#ORIG(?Prompt2)
           STRING(@s50),AT(92,18,250,10),USE(L:FromLayout),FONT(,,COLOR:Navy,,CHARSET:ANSI),#ORIG(?String1)
