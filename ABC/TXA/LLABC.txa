[EMBED]
EMBED %WindowManagerMethodCodeSection
[INSTANCES]
WHEN 'Init'
[INSTANCES]
WHEN '(),BYTE'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 7501
PROPERTY:END
LLL_:Ref = P:Ref
Access:LL_Replay.Tryfetch(LLL_:RefKey)       !Get original record to copy from
L:FromLayout = LLL_:Description
L:OldFileName = LLL_:FileName
[END]
[END]
[END]
EMBED %ControlEventHandling
[INSTANCES]
WHEN '?OK'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
Access:LL_Replay.PrimeRecord(1)        !Prime a new record - do not clear down original so all fields get copied (except Ref)
LLL_:FileName = 'LT' & LLL_:Ref
LLL_:Description = NewDescription
LLL_:FactoryDefault = false            !If this is a copy ift cannot be a factory default

Case Clip(LLL_:Type)
of 'List'
  !****** Form File
  L:FromFileName = Clip(L:OldFileName) & '.lst'
  L:ToFileName = Clip(LLL_:FileName) & '.lst'
  Copy(L:FromFileName,L:ToFileName)

  !****** Printer Definition File
  L:FromFileName = Clip(L:OldFileName) & '.lsp'
  L:ToFileName = Clip(LLL_:FileName) & '.lsp'
  Copy(L:FromFileName,L:ToFileName)

  !****** Sketch for file open dialog
  L:FromFileName = Clip(L:OldFileName) & '.lsv'
  L:ToFileName = Clip(LLL_:FileName) & '.lsv'
  Copy(L:FromFileName,L:ToFileName)
of 'Card'
  !****** Form File
  L:FromFileName = Clip(L:OldFileName) & '.crd'
  L:ToFileName = Clip(LLL_:FileName) & '.crd'
  Copy(L:FromFileName,L:ToFileName)

  !****** Printer Definition File
  L:FromFileName = Clip(L:OldFileName) & '.crp'
  L:ToFileName = Clip(LLL_:FileName) & '.crp'
  Copy(L:FromFileName,L:ToFileName)

  !****** Sketch for file open dialog
  L:FromFileName = Clip(L:OldFileName) & '.crv'
  L:ToFileName = Clip(LLL_:FileName) & '.crv'
  Copy(L:FromFileName,L:ToFileName)

of 'Label'
  !****** Form File
  L:FromFileName = Clip(L:OldFileName) & '.lbl'
  L:ToFileName = Clip(LLL_:FileName) & '.lbl'
  Copy(L:FromFileName,L:ToFileName)

  !****** Printer Definition File
  L:FromFileName = Clip(L:OldFileName) & '.lbp'
  L:ToFileName = Clip(LLL_:FileName) & '.lbp'
  Copy(L:FromFileName,L:ToFileName)

  !****** Sketch for file open dialog
  L:FromFileName = Clip(L:OldFileName) & '.lbv'
  L:ToFileName = Clip(LLL_:FileName) & '.lbv'
  Copy(L:FromFileName,L:ToFileName)
end
Access:LL_Replay.Insert()
[GROUP]
PRIORITY 5000
INSTANCE 6
[END]
[END]
[END]
