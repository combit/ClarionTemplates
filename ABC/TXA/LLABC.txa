PRIORITY 8001
PROPERTY:END
if GetIni('List and Label','Developer','','Solace.ini') = 'Yes' then
  Unhide(?Insert)
  Unhide(?Change)
  Unhide(?Delete)
  ?Browse:1{Prop:Format} = '13C|M~FD~@s1@#4#149L(2)|M~Layout Description~@s60@#1#55L(2)|M~File Name~@s80@#3#16L(2)|M~Type~@s5@#2#'
  ?List{Prop:Format} = '24R(2)|M~Ref~C(0)@n6@#2#200L(2)|M~Area~@s50@#1#'
end
                                 
[END]
