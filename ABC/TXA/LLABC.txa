WHEN 'TakeNewSelection'
[INSTANCES]
WHEN '()'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5001
PROPERTY:END
if clip(LLL_:FileName) <> '' then
  Case(clip(LLL_:Type))
  of 'List'
    NewSketch = clip(LL_GlobalPath) & clip(LLL_:FileName) & '.lsv'
  of 'Label'
    NewSketch = clip(LL_GlobalPath) & clip(LLL_:FileName) & '.lbv'
  of 'Card'
    NewSketch = clip(LL_GlobalPath) & clip(LLL_:FileName) & '.crv'
  end

  Copy(NewSketch,'Sketch.bmp')
  ?Image1{Prop:Text} = ''
  ?Image1{Prop:Text} = 'Sketch.bmp'
  UnHide(?Image1)
  Display(?Image1)
else
  Hide(?Image1)
end
[END]
[END]
[END]
[END]
