               #LINK(?NewDescription)
           ENTRY(@s60),AT(92,40,244,12),USE(NewDescription),REQ,CAP,#ORIG(RPL:Description),#LINK(?RPL:Description:Prompt)
         END
       END
       BUTTON('OK'),AT(204,72,45,14),USE(?OK),DEFAULT,#SEQ(2),#ORIG(?OK)
       BUTTON('Cancel'),AT(252,72,45,14),USE(?Cancel),#SEQ(3),#ORIG(?Cancel)
       BUTTON('Help'),AT(300,72,45,14),USE(?Help),STD(STD:Help)
     END

